{
    "name": "sprunka/common-routes",
    "description": "This is an API stub with generators and routes that I use in most APIs.",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Sean Prunka",
            "email": "sprunka@gmail.com"
        },
        {
            "name": "Vampyre Bytes",
            "email": "admin@vampyrebytes.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "slim/slim": "^4.5",
        "slim/psr7": "^1.2",
        "php-di/php-di": "dev-master",
        "selective/basepath": "dev-master"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "CommonRoutes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommonRoutes\\Test\\": "tests/"
        }
    }
}